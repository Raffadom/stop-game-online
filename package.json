{
  "name": "stop-game-online",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd stop-game-frontend && npm run test:unit",
    "test:backend": "cd stop-game-backend && npm run test:unit",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:ci": "cypress run --browser chrome --headless --reporter json --reporter-options output=cypress/results/results.json",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "install:all": "npm ci && cd stop-game-frontend && npm ci && cd ../stop-game-backend && npm ci",
    "build:frontend": "cd stop-game-frontend && npm run build",
    "start:backend": "cd stop-game-backend && npm start",
    "dev:frontend": "cd stop-game-frontend && npm run dev",
    "dev:backend": "cd stop-game-backend && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raffadom/stop-game-online.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raffadom/stop-game-online/issues"
  },
  "homepage": "https://github.com/Raffadom/stop-game-online#readme",
  "description": "",
  "devDependencies": {
    "cypress": "^15.0.0",
    "wait-on": "^7.2.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "firebase-admin": "^13.4.0"
  }
}
